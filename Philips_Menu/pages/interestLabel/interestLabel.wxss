/**product.wxss**/
/* page{background-color:#E6E6E6;}   */
.philipsLogo {
  width:153rpx;
height:78rpx;
margin-left: 17rpx;

}
.product{
  width:100%;
  height: 100%;
  flex-direction: column;
  box-sizing: border-box;
  position: relative;
}
.top{
  width: 100%;
  /* height: 200rpx; */
}
.headerTitle {
  margin-top: 60rpx;
}
.headerTitle text{
  letter-spacing: 0;
  color: #333333;
  font-weight: 500;
  line-height:65rpx;
font-size:46rpx;
}
.headerTitle .subTitle{
  display: block;
  letter-spacing: 0;
  color: #999999;
  font-weight: 400;
  font-size:30rpx;
  line-height: 42rpx;
  margin:10rpx auto 41rpx;
}
.productContent{
  width:100%;
  height: auto;
}
.productWrap{
  /* width: 678rpx; */
  width: 690rpx;
  height:auto;
  margin: 0 auto 91rpx;
}
.productList{
  width: 33.33%;
	/* height: 210rpx; */
	background-color: #ffffff;
  margin-top: 30rpx;
  text-align: center;
}

.productImg{
  width:220rpx;
height:80rpx;
border-radius:45rpx;
border:1px solid rgba(211,211,211,1);
  margin: 0 auto;line-height: 80rpx;
  position: relative;
}
.productImg text{
  color: #333333;
  font-weight:500;
}
.productImg.active{
  border:2rpx solid rgba(91,153,30,1);
}
.productImg.active text{
  color: #5B991E;
}
.productImg image{
  position: absolute;
  top: 0;
  right: 0;
  width: 34rpx;
  height: 34rpx;
}
.btn{
text-align: center
}
.btn text{
  width:690rpx;
  height:88rpx;
  line-height:88rpx;
  margin: auto;
  border-radius:4rpx;
  display: block;
  font-size:32rpx;
font-weight:500;
color:rgba(255,255,255,1);
}
.btn .btn1{
  background:rgba(230,230,230,1);
}
.btn .btn2{
  background:rgba(91,153,30,1);
}