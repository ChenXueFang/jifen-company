.Box{
  padding-bottom: 50rpx;
}
.swiper-tab {
  width: 100%;
  margin: auto;
  text-align: center;
  margin-bottom: 14rpx;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  background-color: #f6f6f6;
  justify-content: space-around;
}
.swiper-tab-item {
  font-size: 30rpx;
  font-weight: 500;
  color: #9e9e9e;
}
.active {
  font-size: 32rpx;
  color: #5da0de;
  border-bottom: 6rpx solid #5da0de;
}
.show {
  display: block;
  width: 100%;
  margin: auto;
}
.hidden {
  display: none;
}
.equipmentBox{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 335px;
  padding: 70rpx 0;
  margin: 0 auto;
  border-radius: 4px;
  border: solid 1px #e0e0e0;
  background-color: #f7f7f7;
  margin-top: 26rpx;
  margin-bottom: 26rpx;
}
.equipmentBox2{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 335px;
  padding: 70rpx 0;
  margin: 0 auto;
  border-radius: 4px;
  border: solid 1px #e0e0e0;
  background-color: #f7f7f7;
}
.productBox>.equipmentBox2:nth-child(1){
  border-bottom: none;
}
.productBox>.equipmentBox2:nth-child(2){
  padding-top: 0;
  border-top: none;
}
.imgBox{
  width: 50%;
  text-align: center;
  /* padding: 0 56rpx;
  box-sizing: border-box; */
}
.equipmentImg{
  width: 234rpx;
  height: 130rpx;
}
.equipmentImg2{
  width: 224rpx;
  height: 312rpx;
}
.equipmentImg3{
  width: 234rpx;
  height:165rpx;
}
.equipmentInfo{
  width: 50%;
  font-size: 32rpx;
  font-weight: 500;
  color: #484848;
}
.name{
  width: 270rpx;
  margin-bottom: 8rpx;
}
.intro{
  width: 270rpx;
  font-size: 30rpx;
  color: #a7a7a7;
  margin-top: 20rpx;
}
.search{
  width: 335px;
  height: 32px;
  margin: 0 auto;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  border-radius: 4px;
  border: solid 2rpx #e0e0e0;
  background-color: #ffffff;
  color: #b2b2b2;
  margin-top: 38rpx;
  margin-bottom: 46rpx;
}
.icon-search1{
  margin-top: 7rpx;
  color: #333333;
  position: absolute;
  right: 50rpx;
}
.searchfont{
  margin-left: 20rpx;
}
.newCon{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 25rpx 46rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e0e0e0;
}
.newslist>.newCon:first-child{
  border-top: 2rpx solid #e0e0e0;
}
.newslist>.newCon2:first-child{
  border-top: none;
}
.newZi{
  width: 472rpx;
}
.newImg{
  width: 128rpx;
  height: 128rpx;
  border-radius: 5rpx;
}
.newTime{
  text-align: center;
  margin-top: 32rpx;
  color: #a3a3a3;
}
.VIPImgBox{
  text-align: center;
}
.VIPImg{
  width: 510rpx;
  height: 406rpx;
  margin: 60rpx 0;
}
.vipTitle{
  text-align: center;
  font-size: 42rpx;
  font-weight: 500;
  color: #a06d22;
  margin-bottom: 30rpx;
}
.inviteBtn{
  width: 220rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0 auto;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  font-weight: 500;
  border-radius: 11.5px;
  background-color: #dab37a;
}
.vipIntro{
  width: 576rpx;
  margin: 0 auto;
  font-size: 32rpx;
  color: #353535;
  margin-top: 80rpx;
}
.passTime{
  color: #a3a3a3;
}
.listIntro{
  color: #c7c7c7;
  margin-top: 47rpx;
  padding: 0 38rpx;
}





