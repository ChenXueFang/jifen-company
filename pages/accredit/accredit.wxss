/* pages/accredit.wxss */
page{background-color:#ffffff;}  
.accredit{
  width:100%;
  height: 100%;
  flex-direction: column;
  box-sizing: border-box;
  position: relative;
  text-align: center;
}
.philipsLogo {
  position: relative;
  z-index: 10;
}
.bg{
  width: 100%;
  height: 1000rpx;
  position: absolute;
  top:0;
  left:0;
}
.title{
  font-size: 34rpx;
  color: #333;
  font-weight: 600;
  position: absolute;
  top:420rpx;
  left:0;
  right:0;
  margin: 0 auto;
  z-index: 10;
}
.accredit button{
  width: 88%;
  height: 80rpx;
  color:#fff;
  line-height: 80rpx;
	background-color: #0189c5!important;
	border-radius: 10rpx;
  font-size: 32rpx;
  position: absolute;
  top:590rpx;
  left: 6%;
}