/* 登录注册模态框 */

.modalBox .modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modalBox .modal-dialog {
  width: 620rpx;
  height: 520rpx;
  overflow: hidden;
  position: fixed;
  top: 28%;
  left: 9%;
  z-index: 9999;
  background: #fff;
  border-radius: 10rpx;
}
.modalBox .modalCon{
  width: 525rpx;
  height: 385rpx;
  top: 38%;
  left: 16%;
}

.modalBox .modal-title {
  padding-top: 50rpx;
  text-align: center;
}

.modalBox .modal-title>image {
  width: 122rpx;
  height: 96rpx;
}

.modalBox .modal-content {
  padding: 35rpx 0;
}
.modal-conTitle{
  font-weight: bold;
  text-align: center;
  margin-bottom: 20rpx;
  font-size: 35rpx;
}
.modalBox .modal-input {
  width: 509rpx;
  height: 78rpx;
  margin: 0 auto;
  font-size: 32rpx;
  text-align: left;
  color: #333;
}
.modalBox .modal-input2 {
  text-align: center;
}

.modalBox .modal-footer {
  display: flex;
  justify-content: space-around;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
  margin-top: 108rpx;
}
.modalBox .modal-footer2 {
  display: flex;
  justify-content: space-around;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
  margin-top: -14rpx;
}
.modalBox .btn-cancel,.modalBox .btn-confirm {
  width: 250rpx;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  border-radius: 33rpx;
  font-size: 32rpx;
  color: #fff;
}
.modalBox .btn-cancel {
  background-color: #c5c5c5;
}
.modalBox .btn-confirm {
  background-color: #b64c9b;
}
.modalBox .btn-cancel2,.modalBox .btn-confirm2 {
  width: 217rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 33rpx;
  font-size: 31rpx;
  color: #fff;
}
.modalBox .btn-cancel2 {
  background-color: #c5c5c5;
}
.modalBox .btn-confirm2 {
  background-color: #b64c9b;
}
