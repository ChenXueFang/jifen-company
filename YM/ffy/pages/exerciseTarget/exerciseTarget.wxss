page{
  height: 100%;
  background-color: #F4F4F4;
}
#container{
  width: 100%;
}
.targetMain{
  width: 90%;
  margin: auto;
  text-align: center;
}
.targetpic{
  width: 200rpx;
  height: 200rpx;
  margin-top: 50rpx;
}
.section{
  margin-top: 50rpx;
  width: 96%;
  background-color: #FFF;
  text-align: left;
  border-radius: 5rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 30rpx;
  position: relative;
}
.picker{
  position: absolute;
  right: 30rpx;
  top: 0;
}
.targetNumger{
  margin-top: 10rpx;
  background-color: #FFF;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 30rpx;
  border-radius: 5rpx;
   width: 96%;
  text-align: left;
}
.sumbitBtn{
  color: #FFF;
  width: 33%;
  margin: auto;
  margin-top: 50rpx;
}
.sumbitBtn text{
  display: block;
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background-color: #B64C9B;
}